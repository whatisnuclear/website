  title = {Impact Limits of Partitioning and Transmutation Scenarios on the Radiotoxicity of Actinides in Radioactive Waste},
  author = {Magill, J. and Berthou, V. and Haas, D. and Galy, J. and Schenkel, R. and Wiese, H.-W. and Heusener, G. and Tommasi, J. and Youinou, G.},
