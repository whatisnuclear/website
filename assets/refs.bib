@article{cumminsDesignEvolutionPWRs2018,
  title = {Design Evolution of {{PWRs}}: {{Shippingport}} to Generation {{III}}+},
  shorttitle = {Design Evolution of {{PWRs}}},
  author = {Cummins, William Edward and Matzie, Regis},
  year = {2018},
  month = jan,
  journal = {Progress in Nuclear Energy},
  series = {Shippingport 60th {{Anniversary}}: {{A Time}} to {{Take Stock}} of {{Nuclear Energy}}'s {{Status}}},
  volume = {102},
  pages = {9--37},
  issn = {0149-1970},
  doi = {10.1016/j.pnucene.2017.08.008},
