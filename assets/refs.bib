@techreport{vadenSodiumRemovalDisposal1987,
  title = {Sodium Removal and Disposal Handbook},
  author = {Vaden, D. E.},
  year = {1987},
  month = jan,
  number = {ANL-87-30},
  institution = {Argonne National Lab. (ANL), Argonne, IL (United States)},
  doi = {10.2172/713794},
  url = {https://www.osti.gov/biblio/713794},
  urldate = {2025-06-04},
  abstract = {Presently there are various methods for removing sodium from reactor components. Several publications describe cleaning techniques and operating experience at different sodium cleaning facilities. NE Standard F5-9, "Sodium Removal Processes," is the official Department of Energy document that establishes standard procedures for sodium removal with respect to three techniques: water vapor- nitrogen (WVN), alcohol, and evaporation. The Standard does not make a comparison of the three processes to note their advantages and disadvantages, cleaning efficiencies, and which process is best for a particular sodium cleaning operation. This handbook will describe the three cleaning techniques mentioned above, do a comparison of their cleaning effectiveness and application, and discuss ways of processing the sodium metal and sodium compounds produced from component cleaning operations. Sodium removal facilities have already been established at various Liquid Metal Reactor (LMR) installations in the United States and abroad. The available information on sodium cleaning experience at these facilities will be used to develop the sodium removal section of the handbook. The sodium disposal section will cover the various methods available for processing the sodium material (i.e., sodium metal and sodium compounds) produced when LMR components are cleaned. These processes will convert the sodium material to a product that is either suitable for disposal or to be used in neutralizing acidic waste. The primary objective of this handbook is to compile and present information that will assist future operators of LMRs in the selection of a sodium removal facility to suit their particular need and to process the sodium material produced from the component cleaning operations.},
  langid = {english},
  file = {../../home/nick/Zotero/storage/8E5CA3YS/Vaden - 1987 - Sodium removal and disposal handbook.pdf}
}

