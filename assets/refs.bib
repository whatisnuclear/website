@article{marcusConsideringNextGeneration2000,
  title = {Considering the next Generation of Nuclear Power Plants},
  author = {Marcus, Gail H.},
  year = {2000},
  month = jan,
  journal = {Progress in Nuclear Energy},
  series = {Global {{Environment}} and {{Nuclear Energy Systems-3 Proceedings}} of the {{Third International Symposium}}},
  volume = {37},
